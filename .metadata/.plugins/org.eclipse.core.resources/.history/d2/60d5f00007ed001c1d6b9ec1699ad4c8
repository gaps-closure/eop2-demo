#include "Config.hpp"
#include "Provision.hpp"
#include "ProvisionCL.hpp"

using namespace std;

int main(int argc, char **argv)
{
    ProvisionCL cli;
    cli.parseCmdline(argc, argv);

    Config config(NV_VERIFIER);
//    config.setImport(cli.isImport());
    config.parseFile(cli.getConfigFile());

    Provision provision(cli, config);
    provision.run();
}
